{
  "name": "pymockserver-client",
  "version": "1.6.2",
  "description": "PyMockServer client",
  "repository": {
    "type": "git",
    "url": "https://github.com/kudlatyamroth/pymockserver.git",
    "directory": "clients/typescript-node"
  },
  "main": "dist/api.js",
  "typings": "dist/api.d.ts",
  "author": "Karol Fuksiewicz",
  "license": "GPL-3.0",
  "dependencies": {
    "@types/bluebird": "^3.5.30",
    "@types/request": "^2.48.4",
    "bluebird": "^3.7.2",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@types/node": "13.13.5",
    "@types/jest": "^25.2.1",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^25.5.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "./tsconfig.json",
        "diagnostics": false
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "reporters": [
      "default"
    ],
    "testEnvironment": "node",
    "testRegex": "/test/.*.spec.ts",
    "transform": {
      "^.+\\.(ts)$": "ts-jest"
    },
    "verbose": true
  }
}
